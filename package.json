{
  "name": "snapshot-dir",
  "version": "1.1.2",
  "description": "Takes a snapshot of a folder",
  "main": "dist/index.js",
  "typings": "dist/src/index.d.ts",
  "ws": {
    "type": "node"
  },
  "scripts": {
    "build": "yarn run ws b",
    "test": "yarn run ws lint && yarn run ws unit",
