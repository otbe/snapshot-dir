  },
  "keywords": [
    "test",
    "snapshot",
    "dir",
    "folder",
    "hash"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otbe/snapshot-dir.git"
  },
  "author": "Benjamin Otto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otbe/snapshot-dir/issues"
  },
  "homepage": "https://github.com/otbe/snapshot-dir#readme",
  "dependencies": {
    "@types/deep-diff": "^0.0.30",
    "@types/glob": "^5.0.30",
    "@types/md5": "^2.1.32",
    "@types/node": "^7.0.14",
    "deep-diff": "^0.3.6",
    "glob": "^7.1.1",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "@mercateo/ws": "^1.0.1-64",
    "@types/expect": "^1.20.1",
    "@types/mocha": "^2.2.41",
    "expect": "^1.20.2",
    "mocha": "^3.3.0"
  }
